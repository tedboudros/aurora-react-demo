{
  "name": "aurora",
  "version": "0.1.0",
  "main": "electron/index.js",
  "homepage": "./",
  "author": "Ted Boudros",
  "description": "Aurora UI",
  "private": true,
  "dependencies": {
    "@inlet/react-pixi": "^6.0.6",
    "@pixi/filter-adjustment": "^3.1.1",
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "d3-ease": "^2.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "fast-average-color": "^6.3.0",
    "lodash": "^4.17.20",
    "node-sass": "4.14.1",
    "pixi.js": "^5.3.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-awesome-reveal": "^3.5.2",
    "react-dom": "^17.0.1",
    "react-gamepads": "^1.0.0",
    "react-icons": "^4.1.0",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.1",
    "react-spring": "^8.0.27",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "dev": "concurrently -k \"yarn start\" \"yarn electron\"",
    "electron": "wait-on tcp:23000 && electron-forge start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "react-scripts build && electron-forge package",
    "make": "react-scripts build && electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "concurrently": "^5.3.0",
    "electron": "^11.1.1",
    "electron-devtools-installer": "^3.1.1",
    "wait-on": "^5.2.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "Aurora"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "aurora"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
